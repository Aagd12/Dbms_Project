    center 
        h4 Import data in ticket
    <form class="ticket" action="/insert" method="POST">
        <div class="for">
            <input type="int" name="t_id" placeholder="t_id">
            <input type="text"name="t_date" placeholder="t_date">
            <input type="int" name="t_amount" placeholder="t_amount">
            <input type="int" name="p_id" placeholder="p_id">
            <button type="submit" class="btn btn-success">Insert</button>
        </div>   
    </form>
     center 
        h4 Export data from ticket
     <form class="ticket" action="/delete" method="POST">
        <div class="for">
            <input type="number" name="t_id" placeholder="t_id">
             <button type="submit" class="btn btn-danger">Delete</button>
        </div>
            
    </form>
     <center>
     h4 update ticket_id
    </center>
     <form class="ticket" action="/updatet_id" method="POST">
        <div class="for">
            <input type="int" name="prv" placeholder="privious value">
            <input type="int" name="set" placeholder="new updatedvalue">  
            <button type="submit" class="btn btn-primary">update</button>
        </div> 
    </form> 
     <center>
     h4 update ticket_date
    </center>
     <form class="ticket" action="/updatet_date" method="POST">
        <div class="for">
            <input type="text" name="prv" placeholder="privious value">
            <input type="text" name="set" placeholder="new updatedvalue"> 
            <button type="submit" class="btn btn-success">update</button>
        </div>  
       
       
    </form> 
     <center>
     h4 update ticket_amount
    </center>
     <form class="ticket" action="/updatet_amt" method="POST">
        <div class="for">
            <input type="int" name="prv" placeholder="privious value">
            <input type="int" name="set" placeholder="new updatedvalue">  
              <button type="submit" class="btn btn-primary">update</button>
        </div>
       
    </form> 
     <center>
     h4 update passenger_id
    </center>
     <form class="ticket" action="/updatet_pass" method="POST">
        <div class="for">
            <input type="int" name="prv" placeholder="privious value">
            <input type="int" name="set" placeholder="new updatedvalue"> 
              <button type="submit" class="btn btn-success">update</button> 
        </div>
       
    </form> 
     <center>
     h4 Exports
    </center>
     <form class="ticket" action="/exports" method="POST">
        <div class="for">
            <input type="test" name="set" placeholder="file name">  
              <button type="submit" class="btn btn-primary">EXPORTS</button>
        </div>
       
    </form> 
     <center>
     h4 Import
    </center>
     <form class="ticket" action="/import" method="POST">
        <div class="for">
            <input type="test" name="set" placeholder="file name">  
              <button type="submit" class="btn btn-primary"> Import</button>
        </div>
       
    </form> 
    <form class="ticket" action="/view" method="POST">
        <div class="query">
            <button type="submit" class="btn btn-primary">View table </button>
        </div>
    </form> 

